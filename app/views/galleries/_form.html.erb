<%= form_for([current_user, @gallery]) do |f| %>
	<div class="field">
		<%= f.label      	     :title %><br>
		<%= f.text_field 	     :title %>
	</div>
	<div class="field">
		<%= f.label            :images_files %><br>
		<%= f.attachment_field :images_files, multiple: true, presigned: true, direct: true %>
	</div>
	<div class="actions">
		<%= f.submit %>
	</div>
<% end %>
